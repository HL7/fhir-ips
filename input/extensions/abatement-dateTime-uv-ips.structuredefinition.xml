<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="abatement-dateTime-uv-ips"/>
  <url value="http://hl7.org/fhir/uv/ips/StructureDefinition/abatement-dateTime-uv-ips"/>
  <name value="AbatementDateTimeUvIps"/>
  <title value="Condition abatement date"/>
  <status value="active"/>
  <description value="Extension defined by the IPS project to describe the abatement date for allergies"/>
  <kind value="complex-type"/>
  <abstract value="false"/>
  <context>
    <type value="element"/>
    <expression value="AllergyIntolerance"/>
  </context>
  <type value="Extension"/>
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Extension"/>
  <derivation value="constraint"/>
  <differential>
		<element id="Extension:abatement-dateTime-uv-ips">
			<path value="Extension"/>
      <mustSupport value="true"/>
		</element>
    <element id="Extension:abatement-dateTime-uv-ips.url">
      <path value="Extension.url"/>
      <fixedUri value="http://hl7.org/fhir/uv/ips/StructureDefinition/abatement-dateTime-uv-ips"/>
      <mustSupport value="true"/>
    </element>
    <element id="Extension:abatement-dateTime-uv-ips.valueDateTime">
      <path value="Extension.valueDateTime"/>
      <type>
        <code value="dateTime"/>
      </type>
      <mustSupport value="true"/>
    </element>
  </differential>
</StructureDefinition>