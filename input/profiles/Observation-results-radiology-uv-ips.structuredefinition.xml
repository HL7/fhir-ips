				<valueSet value="http://hl7.org/fhir/uv/ips/ValueSet/results-radiology-numobs-gps-dicom-uv-ips"/>
			</binding>
		</element>
		<element id="Observation.component:numericRatioMeasurement.value[x]">
			<path value="Observation.component.value[x]"/>
